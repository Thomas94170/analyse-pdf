generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Document {
  id            String         @id @default(uuid())
  filename      String
  originalName  String
  url           String
  type          DocumentType   @default(AUTRE)
  status        DocumentStatus @default(IN_PROGRESS)
  textExtracted String?
  createdAt     DateTime       @default(now())
}

enum DocumentType {
  FACTURE
  CERFA
  AUTRE
}

enum DocumentStatus {
  IN_PROGRESS
  VALIDATED
  REJECTED
}
